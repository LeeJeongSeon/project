create table book (
book_id varchar2(50) not null primary key,
book_name varchar2(200) not null,
book_img varchar2(50),
book_genre varchar2(50) not null,
book_content clob,
book_publisher varchar2(50),
book_author varchar2(50),
book_counter number default 0,
book_check number default 0 
);

--시퀀스 
create sequence book_seq
start with 1
increment by 1;

insert into book(book_id,book_name,book_img,book_genre,book_content,book_publisher,book_author) 
